<div class="block">
    <h2 class="no-margin uppercase">Research</h2>
</div>
{{#cv.research}}
    <div class="block">
        <h3>
            {{position}}
            <span class="right smaller">
                {{formatDateRange startDate endDate}}
            </span>
        </h3>
        <h4>{{location}}</h4>
        <ul>
            {{#each highlights}}
                <li>{{this}}</li>
            {{/each}}
        </ul>
    </div>
{{/cv.research}}
